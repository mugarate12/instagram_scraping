{
  "name": "portfolio-scraping-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "initialize-database": "npm run build&&node build/src/database/init.js",
    "test": "run-script-os",
    "test:win32": "SET NODE_ENV=test&&npm run initialize-database&&jest --forceExit",
    "test:darwin:linux": "export NODE_ENV=test&&npm run initialize-database&&jest",
    "dev": "run-script-os",
    "dev:win32": "SET NODE_ENV=development&&npm run initialize-database&&nodemon --exec ts-node --files src/server.ts",
    "dev:darwin:linux": "export NODE_ENV=development&&npm run initialize-database&&nodemon --exec ts-node --files src/server.ts",
    "start": "run-script-os",
    "start:win32": "SET NODE_ENV=production&&node build/src/server.js",
    "start:darwin:linux": "export NODE_ENV=production&&node build/src/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "moment-timezone": "^0.5.34",
    "mysql2": "^2.3.3",
    "puppeteer": "^13.4.1",
    "run-script-os": "^1.1.6",
    "sequelize": "^6.17.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@types/cors": "^2.8.12",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/puppeteer": "^5.4.5",
    "@types/supertest": "^2.0.11",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}
